{
  "syntax": {
    "comment_prefix": "//",
    "block_comment": "c",
    "if_open": "if ({cond}) {",
    "else_open": "} else {",
    "while_open": "while ({cond}) {",
    "function_open": "{ret} {name}({args}) {",
    "class_open": "struct {name} : public PyObj {",
    "identifiers": {
      "reserved_words": [
        "alignas",
        "alignof",
        "asm",
        "auto",
        "break",
        "case",
        "catch",
        "char",
        "class",
        "const",
        "constexpr",
        "continue",
        "default",
        "delete",
        "do",
        "double",
        "else",
        "enum",
        "extern",
        "float",
        "for",
        "goto",
        "if",
        "inline",
        "int",
        "long",
        "namespace",
        "new",
        "operator",
        "private",
        "protected",
        "public",
        "register",
        "return",
        "short",
        "signed",
        "sizeof",
        "static",
        "struct",
        "switch",
        "template",
        "this",
        "throw",
        "try",
        "typedef",
        "typename",
        "union",
        "unsigned",
        "virtual",
        "void",
        "volatile",
        "while"
      ],
      "rename_prefix": "py_"
    }
  }
}
