{
  "syntax": {
    "comment_prefix": "//",
    "block_comment": "c",
    "block_close": "}",
    "if_no_brace": "if ({cond})",
    "else_no_brace": "else",
    "if_open": "if ({cond}) {",
    "else_open": "} else {",
    "while_open": "while ({cond}) {",
    "scope_open": "{",
    "scope_exit_open": "auto {guard} = py_make_scope_exit([&]() {",
    "try_open": "try {",
    "catch_open": "catch (const ::std::exception& {name}) {",
    "for_range_open": "for ({type} {target} = {start}; {cond}; {inc})",
    "for_each_unpack_open": "for (auto {iter_tmp} : {iter})",
    "for_each_auto_ref_open": "for (auto& {target} : {iter})",
    "for_each_typed_open": "for ({type} {target} : {iter})",
    "for_open_block": "{header} {",
    "pass_stmt": "/* pass */",
    "break_stmt": "break;",
    "continue_stmt": "continue;",
    "swap_stmt": "::std::swap({left}, {right});",
    "raise_default": "throw ::std::runtime_error(\"raise\");",
    "raise_expr": "throw {exc};",
    "expr_stmt": "{expr};",
    "return_void": "return;",
    "return_value": "return {value};",
    "annassign_member_noinit": "{target};",
    "annassign_member_init": "{target} = {value};",
    "annassign_decl_noinit": "{type} {target};",
    "annassign_decl_init": "{type} {target} = {value};",
    "annassign_assign": "{target} = {value};",
    "augassign_decl_init": "{type} {target} = {value};",
    "augassign_inc": "{target}++;",
    "augassign_dec": "{target}--;",
    "augassign_floordiv": "{target} = py_floordiv({target}, {value});",
    "augassign_apply": "{target} {op} {value};",
    "function_open": "{ret} {name}({args}) {",
    "ctor_open": "{name}({args}) {",
    "dtor_open": "~{name}() {",
    "class_open": "struct {name}{base_txt} {",
    "class_close": "};",
    "identifiers": {
      "reserved_words": [
        "alignas",
        "alignof",
        "asm",
        "auto",
        "break",
        "case",
        "catch",
        "char",
        "class",
        "const",
        "constexpr",
        "continue",
        "default",
        "delete",
        "do",
        "double",
        "else",
        "enum",
        "extern",
        "float",
        "for",
        "goto",
        "if",
        "inline",
        "int",
        "long",
        "namespace",
        "new",
        "operator",
        "private",
        "protected",
        "public",
        "register",
        "return",
        "short",
        "signed",
        "sizeof",
        "static",
        "struct",
        "switch",
        "template",
        "this",
        "throw",
        "try",
        "typedef",
        "typename",
        "union",
        "unsigned",
        "virtual",
        "void",
        "volatile",
        "while"
      ],
      "rename_prefix": "py_"
    }
  }
}
