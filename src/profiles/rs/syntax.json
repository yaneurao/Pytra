{
  "syntax": {
    "comment_prefix": "//",
    "block_close": "}",
    "if_open": "if {cond} {",
    "else_open": "} else {",
    "while_open": "while {cond} {",
    "for_open": "for {target} in {iter} {",
    "for_range_open": "while {cond} {",
    "expr_stmt": "{expr};",
    "return_void": "return;",
    "return_value": "return {value};",
    "pass_stmt": "// pass",
    "break_stmt": "break;",
    "continue_stmt": "continue;",
    "annassign_decl_noinit": "let mut {target}: {type};",
    "annassign_decl_init": "let mut {target}: {type} = {value};",
    "annassign_assign": "{target} = {value};",
    "assign_decl_init": "let mut {target} = {value};",
    "assign_set": "{target} = {value};",
    "augassign_apply": "{target} {op} {value};",
    "function_open": "fn {name}({args}){ret_txt} {",
    "identifiers": {
      "reserved_words": [
        "as",
        "break",
        "const",
        "continue",
        "crate",
        "else",
        "enum",
        "extern",
        "false",
        "fn",
        "for",
        "if",
        "impl",
        "in",
        "let",
        "loop",
        "match",
        "mod",
        "move",
        "mut",
        "pub",
        "ref",
        "return",
        "self",
        "Self",
        "static",
        "struct",
        "super",
        "trait",
        "true",
        "type",
        "unsafe",
        "use",
        "where",
        "while",
        "async",
        "await",
        "dyn"
      ],
      "rename_prefix": "py_"
    }
  }
}
